<form bindsubmit="formSubmit" bindreset="formReset">
  <view class="content">
     
     <view class="top">
      兼职申请
    </view>
    

    <view>
      <view class="userName">
        <text class="userNameText">兼职名称</text>
        <input type="text" name="orderPartName" class="userNameInput" placeholder="请输入兼职名称"/>
      </view>
      
      <view class="userName">
        <text class="userNameText">兼职时间</text>
        <input type="text" class="userNameInput" name="userPartTime" placeholder="请输入兼职时间"/>
      </view>


      <view class="userName">
        <text class="userNameText">兼职内容</text>
        <input type="text" class="userNameInput" name="orderPartPlace"  placeholder="请输入简直内容"/>
      </view>

      <view class="userName">
        <text class="userNameText">兼职地点</text>
        <input type="text" class="userNameInput" name="orderPartContent" placeholder="请输入兼职地点"/>
      </view>

      <view class="userName">
        <text class="userNameText"> 联系方式</text>
        <input type="text" class="userNameInput" name="orderPartNumber" placeholder="请输入兼职联系方式"/>
      </view>
      
      <view class="userName">
        <text class="userNameText">薪资待遇</text>
        <input type="text" class="userNameInput" name="orderPartVenue" placeholder="请输入薪资待遇"/>
      </view>


    </view>


    <view class='pagefoot'>
      <button class='btnblock' form-type="submit">提交</button>
    </view>


  </view>
 </form>